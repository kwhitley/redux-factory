{
  "name": "redux-registry",
  "version": "0.0.4",
  "description": "Redux registry for cleaner action definition",
  "main": "lib/index.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src test",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm test -- --watch",
    "check:src": "npm run lint && npm run test",
    "build": "babel -d dist/ src/ && browserify dist/redux-registry.js | uglifyjs > dist/redux-registry.min.js",
    "prepublish": "npm run clean && npm run check:src && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kwhitley/redux-registry.git"
  },
  "keywords": [
    "redux",
    "reducer",
    "state",
    "predictable",
    "functional",
    "immutable",
    "hot",
    "live",
    "replay",
    "flux",
    "elm"
  ],
  "authors": [
    "Kevin R. Whitley <kevin@krwhitley.com> (https://github.com/kwhitley)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwhitley/redux-registry/issues"
  },
  "homepage": "https://github.com/kwhitley/redux-registry/",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.3.15",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.9.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "check-es3-syntax-cli": "0.0.2",
    "cross-env": "^1.0.7",
    "eslint": "^1.10.3",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^3.16.1",
    "expect": "^1.8.0",
    "glob": "^6.0.4",
    "immutable": "^3.8.1",
    "isparta": "^4.0.0",
    "mocha": "^2.5.3",
    "redux": "^3.5.2",
    "rimraf": "^2.3.4",
    "rxjs": "^5.0.0-beta.6",
    "uglifyjs": "^2.4.10",
    "webpack": "^1.9.6"
  },
  "npmName": "redux",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
}
